-- TS 44.060 - d60
-- 12.9 Global Power Control Parameters
-- Global Power Control Parameters IE

< Global Power Control Parameters IE > ::=
	< ALPHA : bit (4) >
	< T_AVG_W : bit (5) >
	< T_AVG_T : bit (5) >
	< Pb : bit (4) >
	< PC_MEAS_CHAN : bit (1) >
	0					-- The value '1' was allocated in an earlier version of the protocol and shall not be used. 
	< N_AVG_I : bit (4) > ;

